-- Dragging (keeping minimal code)
local dragging, miniDragging = false, false
local dragStart, startPos, miniDragStart, miniStartPos

dragHandle.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = mainFrame.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then dragging = false end
		end)
	end
end)

dragHandle.InputChanged:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and dragging then
		local delta = input.Position - dragStart
		mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

miniBtn.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		miniDragging = true
		miniDragStart = input.Position
		miniStartPos = miniBtn.Position
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then miniDragging = false end
		end)
	end
end)

miniBtn.InputChanged:Connect(function(input)
	if (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) and miniDragging then
		local delta = input.Position - miniDragStart
		miniBtn.Position = UDim2.new(miniStartPos.X.Scale, miniStartPos.X.Offset + delta.X, miniStartPos.Y.Scale, miniStartPos.Y.Offset + delta.Y)
	end
end)

local isMinimized = false
minimizeBtn.Activated:Connect(function()
	isMinimized = true
	TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.new(0, 0, 0, 0)}):Play()
	task.wait(0.3)
	mainFrame.Visible = false
	miniBtn.Visible = true
	miniBtn.Size = UDim2.new(0, 0, 0, 0)
	TweenService:Create(miniBtn, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 50, 0, 50)}):Play()
end)

miniBtn.Activated:Connect(function()
	if not miniDragging then
		isMinimized = false
		TweenService:Create(miniBtn, TweenInfo.new(0.2, Enum.EasingStyle.Back, Enum.EasingDirection.In), {Size = UDim2.new(0, 0, 0, 0)}):Play()
		task.wait(0.2)
		miniBtn.Visible = false
		mainFrame.Visible = true
		mainFrame.Size = UDim2.new(0, 0, 0, 0)
		TweenService:Create(mainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {Size = UDim2.new(0, 180, 0, 360)}):Play()
	end
end)

antiKickBtn.Activated:Connect(function()
	antiKickEnabled = not antiKickEnabled
	antiKickBtn.Text = antiKickEnabled and "ðŸ›¡ï¸ Anti-Kick: ON" or "ðŸ›¡ï¸ Anti-Kick: OFF"
	antiKickBtn.BackgroundColor3 = antiKickEnabled and Color3.fromRGB(80, 255, 100) or Color3.fromRGB(255, 80, 80)
end)

-- REAL INVISIBILITY METHODS
local invisConnection = nil
local velocityPart = nil
local originalCFrame = nil

-- METHOD 1: VELOCITY (Constant repositioning - bypasses rendering)
function applyVelocity()
	if not character or not hrp then return end
	
	-- Create invisible part far away
	velocityPart = Instance.new("Part")
	velocityPart.Anchored = true
	velocityPart.CanCollide = false
	velocityPart.Transparency = 1
	velocityPart.Size = Vector3.new(1, 1, 1)
	velocityPart.Position = Vector3.new(0, -1000, 0)
	velocityPart.Parent = workspace
	
	-- Constantly move character to far position then back (so fast others don't see)
	invisConnection = RunService.Heartbeat:Connect(function()
		if not invisEnabled or not character or not hrp then return end
		
		-- Save position
		local currentPos = hrp.CFrame
		
		-- Teleport far away (bypasses rendering for other players)
		hrp.CFrame = CFrame.new(0, 999999, 0)
		
		-- Immediately teleport back
		task.wait()
		hrp.CFrame = currentPos
	end)
	
	statusLabel.Text = "Status: ACTIVE (Velocity)\nMethod: Fast Repositioning\nTools: Functional"
end

-- METHOD 2: VOID (Below FallenPartsDestroyHeight)
function applyVoid()
	if not character or not hrp then return end
	
	originalCFrame = hrp.CFrame
	
	-- Get fallen parts destroy height
	local destroyHeight = workspace.FallenPartsDestroyHeight - 50
	
	-- Move character below destroy height
	invisConnection = RunService.Heartbeat:Connect(function()
		if not invisEnabled or not character or not hrp then return end
		
		-- Keep character below map but above destroy height
		local pos = hrp.Position
		if pos.Y > destroyHeight then
			hrp.CFrame = CFrame.new(pos.X, destroyHeight, pos.Z)
		end
		
		-- Enable noclip
		for _, part in ipairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.CanCollide = false
			end
		end
	end)
	
	-- Initial move
	hrp.CFrame = CFrame.new(hrp.Position.X, destroyHeight, hrp.Position.Z)
	
	statusLabel.Text = "Status: ACTIVE (Void)\nMethod: Below Destroy Height\nTools: Functional"
end

-- METHOD 3: FAKE DEATH (Respawn exploit - advanced)
function applyFakeDeath()
	if not character or not humanoid or not hrp then return end
	
	-- Set health to 0 locally (tricks render system)
	humanoid.Health = 0
	
	-- Wait a frame
	task.wait()
	
	-- Restore health (character appears "dead" to others but alive to you)
	humanoid.Health = humanoid.MaxHealth
	
	-- Hide ragdoll effect
	for _, part in ipairs(character:GetDescendants()) do
		if part:IsA("BasePart") then
			part.Transparency = 1
			part.CanCollide = false
		end
	end
	
	statusLabel.Text = "Status: ACTIVE (Fake Death)\nMethod: Death Exploit\nTools: Functional"
end

-- METHOD 4: NETWORK (Network ownership manipulation)
function applyNetwork()
	if not character or not hrp then return end
	
	-- Set network ownership
	pcall(function()
		hrp:SetNetworkOwner(nil)
	end)
	
	invisConnection = RunService.Heartbeat:Connect(function()
		if not invisEnabled or not character then return end
		
		-- Manipulate all parts
		for _, part in ipairs(character:GetDescendants()) do
			if part:IsA("BasePart") and part ~= hrp then
				-- Set to far position
				pcall(function()
					part:SetNetworkOwner(player)
					part.CFrame = CFrame.new(0, 999999, 0)
				end)
			end
		end
	end)
	
	statusLabel.Text = "Status: ACTIVE (Network)\nMethod: Ownership Trick\nTools: Functional"
end

function enableInvisibility()
	if not character then return end
	
	if currentMethod == "velocity" then
		applyVelocity()
	elseif currentMethod == "void" then
		applyVoid()
	elseif currentMethod == "fakedeath" then
		applyFakeDeath()
	elseif currentMethod == "network" then
		applyNetwork()
	end
end

function disableInvisibility()
	-- Stop connections
	if invisConnection then
		invisConnection:Disconnect()
		invisConnection = nil
	end
	
	-- Clean up velocity part
	if velocityPart then
		velocityPart:Destroy()
		velocityPart = nil
	end
	
	-- Restore position
	if originalCFrame and hrp then
		hrp.CFrame = originalCFrame
		originalCFrame = nil
	end
	
	-- Restore character
	if character then
		for _, part in ipairs(character:GetDescendants()) do
			if part:IsA("BasePart") then
				part.Transparency = 0
				part.CanCollide = true
			end
		end
	end
	
	-- Restore humanoid
	if humanoid and humanoid.Health <= 0 then
		humanoid.Health = humanoid.MaxHealth
	end
	
	statusLabel.Text = "Status: Ready\nMethod: " .. currentMethod:upper() .. "\nTools: Functional"
end

-- Anti-Kick
spawn(function()
	while true do
		wait(2)
		if antiKickEnabled and invisEnabled and humanoid then
			pcall(function()
				humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
				task.wait(0.05)
				humanoid:ChangeState(Enum.HumanoidStateType.Freefall)
			end)
		end
	end
end)

-- Main Toggle
toggleBtn.Activated:Connect(function()
	invisEnabled = not invisEnabled
	
	if invisEnabled then
		toggleBtn.Text = "ðŸ‘» ENABLED"
		toggleBtn.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
		statusDot.BackgroundColor3 = Color3.fromRGB(150, 100, 255)
		enableInvisibility()
	else
		toggleBtn.Text = "ðŸ‘» DISABLED"
		toggleBtn.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
		statusDot.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
		disableInvisibility()
	end
end)

